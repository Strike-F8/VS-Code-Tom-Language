{
  "name": "tom-language",
  "displayName": "Tom Language",
  "description": "Syntax highlighting for Tom Language",
  "version": "0.0.2",
  "publisher": "StrikeF8",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/Strike_F8/vs-code-tom-language-support.git"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tom",
      "aliases": ["Tom", "tom"],
      "extensions": [".toms"],
      "configuration": "./language-configuration/language-configuration.json"
    }],
    "grammars": [{
      "language": "tom",
      "scopeName": "source.tom",
      "path": "./syntaxes/tom.tmLanguage.json"
    }]
  }
}
